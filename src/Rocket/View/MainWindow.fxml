               <RadioButton fx:id="Button_BestCost" mnemonicParsing="false" onMouseClicked="#deltavcostClicked" selected="true" text="Best Cost">
                  <VBox.margin>
                     <Insets bottom="2.0" top="2.0" />
                  </VBox.margin>
               </RadioButton>
               <RadioButton fx:id="Button_BestDeltaV" mnemonicParsing="false" onMouseClicked="#deltavcostClicked" text="Best DeltaV">
                  <VBox.margin>
                     <Insets bottom="2.0" top="2.0" />
                  </VBox.margin>
               </RadioButton>
            </children>
            <opaqueInsets>
               <Insets />
            </opaqueInsets>
         </VBox>
        </GridPane>
      <WebView fx:id="webView" blendMode="MULTIPLY" maxWidth="500.0" prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
      <padding>
