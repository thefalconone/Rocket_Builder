            </Spinner>
            <Label text="Minimum TWR" wrapText="true" GridPane.rowIndex="2" />
            <Slider fx:id="Slider_twrmin" blockIncrement="0.5" majorTickUnit="5.0" max="20.0" minorTickCount="4" onMouseDragged="#mintwrChangedValue" showTickLabels="true" showTickMarks="true" GridPane.rowIndex="3" />
            <Label text="Maximum TWR" wrapText="true" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Slider fx:id="Slider_twrmax" blockIncrement="0.5" majorTickUnit="5.0" max="20.0" minorTickCount="4" onMouseDragged="#maxtwrChangedValue" showTickLabels="true" showTickMarks="true" GridPane.columnIndex="1" GridPane.rowIndex="3" />
