<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.web.*?>

<AnchorPane prefHeight="491.0" prefWidth="657.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Rocket.Controller.MainWindow">
	<MenuBar layoutY="2.0" prefHeight="25.0" prefWidth="600.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
		<Menu mnemonicParsing="false" text="File">
			<MenuItem mnemonicParsing="false" text="Save Rocket" />
         <MenuItem mnemonicParsing="false" onAction="#options" text="Options" />
         <MenuItem mnemonicParsing="false" text="Close" />
		</Menu>
	</MenuBar>
	<HBox layoutX="29.0" layoutY="39.0" prefHeight="389.0" prefWidth="747.0" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="27.0">
        <GridPane minHeight="170.0" minWidth="150.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="139.0" minWidth="10.0" prefWidth="109.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="95.0" minWidth="10.0" prefWidth="91.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
            <Label text="Importance" GridPane.columnIndex="1" />
            <Label text="DeltaV" GridPane.rowIndex="1" />
            <Slider fx:id="Slider_dv" majorTickUnit="2500.0" max="10000.0" minorTickCount="1" onMouseDragged="#dvChangedValue" showTickLabels="true" showTickMarks="true" GridPane.rowIndex="2" />
            <Spinner fx:id="Spinner_moddeltav" onInputMethodTextChanged="#moddeltavChangedValue" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label text="Cost" GridPane.rowIndex="3" />
            <Slider fx:id="Slider_cost" majorTickUnit="50000.0" max="100000.0" minorTickCount="5" onDragDetected="#costChangedValue" showTickLabels="true" showTickMarks="true" GridPane.rowIndex="4" />
            <Spinner fx:id="Spinner_modcost" onInputMethodTextChanged="#modcostChangedValue" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Label text="TWR" GridPane.rowIndex="5" />
            <Slider fx:id="Slider_twr" blockIncrement="0.5" majorTickUnit="5.0" max="20.0" minorTickCount="4" onDragDetected="#twrChangedValue" showTickLabels="true" showTickMarks="true" GridPane.rowIndex="6" />
            <Spinner fx:id="Spinner_modtwr" onInputMethodTextChanged="#modtwrChangedValue" GridPane.columnIndex="1" GridPane.rowIndex="5" />
        </GridPane>
        <VBox minWidth="100.0" prefHeight="200.0" spacing="10.0">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
            <HBox.margin>
                <Insets />
            </HBox.margin>
            <Label text="Max number of stages" wrapText="true" />
            <Spinner fx:id="Spinner_nbmaxstages" />
            <Label text="Max number of fuel tanks/stage" wrapText="true" />
            <Spinner fx:id="Spinner_nbmaxft" />
            <Button mnemonicParsing="false" onAction="#buttonBuildClicked" prefWidth="200.0" text="Build" VBox.vgrow="ALWAYS">
                <VBox.margin>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                </VBox.margin>
            </Button>
        </VBox>
      <WebView fx:id="webView" blendMode="MULTIPLY" maxWidth="500.0" prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
      <padding>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
      </padding>
	</HBox>
</AnchorPane>
