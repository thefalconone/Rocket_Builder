                    <SpinnerValueFactory.IntegerSpinnerValueFactory initialValue="5" max="10" min="0" />
                </valueFactory>
            </Spinner>
            <Label text="Max number of fuel tanks/stage" wrapText="true" GridPane.columnIndex="1" GridPane.rowIndex="5" />
            <Spinner fx:id="Spinner_nbmaxft" onMousePressed="#nbmaxftChangedValue" GridPane.columnIndex="1" GridPane.rowIndex="6">
