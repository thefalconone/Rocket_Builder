            </Spinner>
            <Button mnemonicParsing="false" onAction="#buttonBuildClicked" prefHeight="50.0" prefWidth="200.0" text="Build" GridPane.columnIndex="3" GridPane.rowIndex="5" GridPane.rowSpan="2">
