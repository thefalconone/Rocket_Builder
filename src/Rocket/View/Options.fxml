<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="300.0" prefWidth="599.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Rocket.Controller.Options">
	<VBox prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
		<GridPane minHeight="80.0" VBox.vgrow="ALWAYS">
			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
			</rowConstraints>
			<VBox.margin>
				<Insets />
			</VBox.margin>
			<Label text="Population size" wrapText="true">
				<padding>
					<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
				</padding>
			</Label>
			<Label text="Number of generations" wrapText="true" GridPane.columnIndex="2">
				<padding>
					<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
				</padding>
			</Label>
			<Label text="Number of individuals killed/generation" wrapText="true" GridPane.rowIndex="1">
				<padding>
					<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
				</padding>
			</Label>
			<Label text="Number of individuals mutated/generation" wrapText="true" GridPane.columnIndex="2" GridPane.rowIndex="1">
				<padding>
					<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
				</padding>
			</Label>
			<Spinner fx:id="Spinner_nbpop" GridPane.columnIndex="1" />
			<Spinner fx:id="Spinner_ratiokill" GridPane.columnIndex="1" GridPane.rowIndex="1" />
			<Spinner fx:id="Spinner_nbgen" GridPane.columnIndex="3" />
			<Spinner fx:id="Spinner_nbmut" GridPane.columnIndex="3" GridPane.rowIndex="1" />
		</GridPane>
		<HBox alignment="BOTTOM_RIGHT" spacing="10.0">
			<padding>
				<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
			</padding>
			<Button mnemonicParsing="false" onAction="#optionsOkPressed" text="Ok" />
			<Button mnemonicParsing="false" onAction="#optionsCancelPressed" text="Cancel" />
		</HBox>
	</VBox>
</AnchorPane>
