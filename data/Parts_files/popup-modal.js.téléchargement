function cancelThisModal(){setCookie(cookieLoginName,'');var cover=document.getElementById('cover');cover.parentNode.removeChild(cover);var myModal=document.getElementById('my-js-modal-2');myModal.parentNode.removeChild(myModal);bindLoginEvent();}