function setCookie(cname,cvalue){var d=new Date();d.setTime(d.getTime()+31536000000);document.cookie=cname+"="+cvalue+";path=/;expires="+d.toUTCString();}
function getCookie(cname){var name=cname+"=";var decodedCookie=decodeURIComponent(document.cookie);var ca=decodedCookie.split(';');console.log(decodedCookie);for(var i=0;i<ca.length;i++){var c=ca[i];while(c.charAt(0)==' '){c=c.substring(1);}
if(c.indexOf(name)==0){return c.substring(name.length,c.length);}}
return "";}
var cookieLoginName='ksp.mediawiki.login';document.addEventListener('DOMContentLoaded',function(){var checkError=$('#userloginForm .error').length;console.log(checkError);if(checkError>0){setCookie(cookieLoginName,'');}
bindLoginEvent();});function bindLoginEvent(){$('#userloginForm form').submit(function(event){var cookieValue=getCookie(cookieLoginName);var username=$('#wpName1').val();if(cookieValue==username){return true;}else{openLoginModal();return false;}});}
function openLoginModal(){var createButton=$('#wpCreateaccount').length;if(createButton>0){createButton=true;}else{createButton=false;}
var modal=document.createElement("div"),modalStyle=document.createElement("style"),modalCSS='',modalClose='',theBody=document.getElementsByTagName('body')[0],theHead=document.getElementsByTagName('head')[0];var modalContent="<div>";if(createButton){modalContent+="<p>Your acceptance of the <a target='_blank' href='http://www.take2games.com/privacy/'>Privacy Policy</a> and <a target='_blank' href='http://www.take2games.com/legal/'>Terms of Service</a> is required to create an account.</p>";}else{modalContent+="<p>Please note that the <a target='_blank' href='http://www.take2games.com/privacy/'>Privacy Policy</a> and <a target='_blank' href='http://www.take2games.com/legal/'>Terms of Service</a> (collectively, the \"Terms\") were updated on March 6, 2018. Please read the agreements. Your acceptance of the Terms is required in order to access to your account.</p>";}
modalContent+="<br><center><button id='js_modal_close_2' class='modal-close'>Yes, I accept.</button><center>";modalContent+='<br><center><small><a href="javascript:void(0);" onclick="cancelThisModal();">No, I don\'t agree</a></small></center>';modalContent+="</div>";modal.setAttribute("class","js-modal");modal.setAttribute("id","my-js-modal-2");modal.innerHTML='<div class="js-modal-inner">'+modalContent+modalClose+'</div>';theBody.appendChild(modal);theBody.innerHTML=theBody.innerHTML+"<div id='cover'></div>"
